		"tailwindcss": "^3.4.14",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"vite-plugin-tailwind-purgecss": "^0.3.3",
		"vitest": "^2.1.4"
	},
	"dependencies": {
		"vite": "^5.4.11"
	},
	"engines": {
		"node": "^20.18",
		"pnpm": "^9"
	},
	"volta": {
		"node": "20.18.0"
